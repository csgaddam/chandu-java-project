<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name>Banking application</display-name>

	<servlet>
		<servlet-name>Adminlogin</servlet-name>
		<servlet-class>com.banking.servlet.AdminServlet</servlet-class>
	</servlet>

	<servlet>
		<servlet-name>fetch</servlet-name>
		<servlet-class>com.banking.servlet.CustomerDetails</servlet-class>
	</servlet>



	<servlet>
		<servlet-name>accdetails</servlet-name>
		<jsp-file>/fetch.jsp</jsp-file>
	</servlet>


	<servlet-mapping>
		<servlet-name>fetch</servlet-name>
		<url-pattern>/servlet1</url-pattern>
	</servlet-mapping>


	<servlet-mapping>
		<servlet-name>Adminlogin</servlet-name>
		<url-pattern>/adminservlet</url-pattern>
	</servlet-mapping>



	<servlet-mapping>
		<servlet-name>accdetails</servlet-name>
		<url-pattern>/customerdetails</url-pattern>
	</servlet-mapping>
	
	<welcome-file-list>
		<welcome-file>login.jsp</welcome-file>
	</welcome-file-list>


</web-app>
